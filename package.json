{
  "name": "tarot-timer-app",
  "version": "1.0.0",
  "description": "🔮 시간별 타로 카드와 함께하는 의미있는 하루",
  "main": "App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "prebuild": "expo prebuild --clean",
    "figma:extract": "tsx src/utils/figma-extraction-cli.ts",
    "figma:extract:watch": "tsx src/utils/figma-extraction-cli.ts --watch",
    "figma:extract:all": "tsx src/utils/figma-extraction-cli.ts --format all --platform universal",
    "figma:extract:web": "tsx src/utils/figma-extraction-cli.ts --format all --platform web",
    "figma:extract:rn": "tsx src/utils/figma-extraction-cli.ts --format all --platform react-native",
    "figma:validate": "tsx src/utils/figma-extraction-cli.ts --validate --format json --verbose",
    "migrate": "tsx src/scripts/migration-cli.ts",
    "migrate:analyze": "tsx src/scripts/migration-cli.ts analyze",
    "migrate:plan": "tsx src/scripts/migration-cli.ts plan",
    "migrate:auto": "tsx src/scripts/migration-cli.ts auto",
    "migrate:dry-run": "tsx src/scripts/migration-cli.ts auto --dry-run",
    "migrate:validate": "tsx src/scripts/migration-cli.ts validate",
    "migrate:status": "tsx src/scripts/migration-cli.ts status",
    "migrate:interactive": "tsx src/scripts/migration-cli.ts interactive",
    "typecheck": "npx tsc --noEmit",
    "build:web": "npx expo export --platform web",
    "build:analyze": "ANALYZE=true npm run build:web",
    "analyze": "npm run build:analyze",
    "validate:web": "node scripts/validate-web-config.js",
    "assets:validate": "node scripts/validate-assets.js",
    "assets:create": "node scripts/create-simple-pngs.js",
    "log:complete": "node scripts/auto-logger.js complete",
    "log:status": "node scripts/auto-logger.js status",
    "work:start": "./scripts/work-tracker.sh start",
    "work:complete": "./scripts/work-tracker.sh complete",
    "work:status": "./scripts/work-tracker.sh status",
    "work:summary": "./scripts/work-tracker.sh summary"
  },
  "dependencies": {
    "@expo-google-fonts/noto-sans-kr": "^0.4.2",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "date-fns": "^3.6.0",
    "expo": "53.0.22",
    "expo-background-fetch": "~13.1.6",
    "expo-constants": "~17.1.7",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.5",
    "expo-sharing": "~13.1.5",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-task-manager": "~13.1.6",
    "immer": "^10.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "^0.20.0",
    "seedrandom": "^3.0.5",
    "typescript": "~5.8.3",
    "uuid": "^11.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@types/inquirer": "^9.0.7",
    "@types/react": "~19.0.10",
    "@types/seedrandom": "^3.0.8",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^12.0.0",
    "inquirer": "^8.2.6",
    "jsdom": "^26.1.0",
    "tsx": "^4.7.0"
  },
  "keywords": [
    "tarot",
    "timer",
    "react-native",
    "expo"
  ],
  "author": "therearenorules",
  "license": "MIT"
}
